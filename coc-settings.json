{
  "coc.preferences.formatOnSaveFiletypes": [
    "Markdown",
    "rmd",
    "python",
    "cucumber",
    "python.django",
    "vue",
    "javascript.jsx",
    "javascript",
    "javascript.tsx",
    "typescript.tsx",
    "typescript"
  ],
  "suggest.timeout": 500,
  "eslint.autoFixOnSave": true,
  "emmet.includeLanguages": { "vue": "html" },
  "git.enableGlobalStatus": false,
  "diagnostic.checkCurrentLine": true,
  "coc.source.around.priority": 3
}
